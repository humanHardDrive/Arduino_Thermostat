
(rules PCB Thermostat_Layout
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout on)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 99)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.2)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 350.0 (type default_HVAC))
    (clear 350.0 (type HVAC_smd))
    (clear 350.0 (type HVAC_"kicad_default"))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND "Net-(BT1-Pad1)" /ProcessorConnections/X1 /ProcessorConnections/X2 "Net-(C3-Pad1)" +9V 3.3V "Net-(J1-Pad3)"
    "Net-(J1-Pad1)" "UNUSED_BTN" "SCHED_BTN" "REMOTE_BTN" "SETTINGS_BTN" "FAN_BTN" "MODE_BTN" "DWN_BTN"
    "UP_BTN" "LCD_LED" "LCD_D7" "LCD_D6" "LCD_D5" "LCD_D4" "LCD_EN" "LCD_RS"
    "LCD_CONT" SDA SCL "UC_RST" "UC_TX" "UC_RX" "Net-(R10-Pad1)" "Net-(TP1-Pad1)"
    "/ProcessorConnections/ANA_TEMP" "EXP_INT" "EXP_RST" "HEAT_ON" "FAN_ON" "COOL_ON" "Net-(J2-Pad3)" "Net-(J2-Pad1)"
    "Net-(J6-Pad15)" "Net-(J7-Pad3)" "Net-(J7-Pad1)" "Net-(J8-Pad3)" "Net-(J8-Pad1)" "/ProcessorConnections/DBG_TX" "/ProcessorConnections/DBG_RX" "Net-(R11-Pad1)"
    "Net-(R12-Pad1)" "RADIO_EN" "RADIO_RST" "Net-(R16-Pad1)" "/Radio/RADIO_PRG_TX" "/Radio/RADIO_PRG_RX" "/Radio/DBG_TX" "/Radio/DBG_RX"
    "Net-(SW3-Pad1)" /Radio/RX /Radio/TX "RADIO_STATUS" "BATT_MON"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class HVAC
    COMMON "FAN_HOT" "COOL_HOT" "HEAT_HOT"
    (clearance_class "kicad_default")
    (via_rule default)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)